<?xml version="1.0" encoding="UTF-8"?>
<staci:acl xmlns:staci="urn:nermal/staci"
		xmlns:contact="urn:nermal/contact">
	<!--	
		Skupina uživatelů
		- ->
	<staci:group name="admins">
		<staci:user ref="taco" />
		<staci:user ref="mira" />
	</staci:group>

	<staci:group name="vyvojari">
		<staci:user ref="taco" />
