		<con:lastname>Fean</con:lastname>
		<con:email>mt@darkmay.cz</con:email>
		<st:ssh type="dsa">ssh-dss  taco@taco.darkmay.cz</st:ssh>
		<st:access>
			<st:permission>read</st:permission>
		</st:access>		

	</st:user>

</st:acl>
