<?xml version="1.0" encoding="UTF-8"?>
<st:acl xmlns:st="urn:nermal/staci"
		xmlns:con="urn:nermal/contact">
	<!--	
		Skupina uživatelů
		- ->
	<st:group name="admins">
		<st:user ref="taco" />
		<st:user ref="mira" />
	</st:group>

	<st:group name="vyvojari">
		<st:user ref="taco" />
