		<staci:user ref="fean" />
	</staci:group>
-->

	<!--
		Definice konkrétních uživatelů
		-->
	<staci:user name="taco">
		<contact:firstname>Martin</contact:firstname>
		<contact:lastname>Takáč</contact:lastname>
		<contact:email>mt@darkmay.cz</contact:email>
		<staci:ssh type="dsa">ssh-dss  taco@taco.darkmay.cz</staci:ssh>


		<!--
			Existují celkem čtyři skupiny práv: 
				R jen pro čtení, 
				RW čtení i zápis, 
				RW+ přidá mazání, 
				– pro zákaz přístupu
				shell: Může se přihlásit
			-->
		<staci:access>
			<staci:permission>read</staci:permission>
			<staci:permission>write</staci:permission>
			<staci:permission>remove</staci:permission>
			<staci:permission>shell</staci:permission>
		</staci:access>		

	</staci:user>

	<!--
		Definice konkrétních uživatelů
		-->
	<staci:user name="mira">
		<contact:email>mf@darkmay.cz</contact:email>
		<staci:ssh type="dsa">ssh-dss  taco@taco.darkmay.cz</staci:ssh>
		<staci:access>
			<staci:permission>read</staci:permission>
			<staci:permission>write</staci:permission>
			<staci:permission>remove</staci:permission>
		</staci:access>		

	</staci:user>

	<!--
		Definice konkrétních uživatelů
		-->
	<staci:user name="fean">
		<contact:firstname>Andreaw</contact:firstname>
		<contact:lastname>Fean</contact:lastname>
		<contact:email>mt@darkmay.cz</contact:email>
		<staci:ssh type="dsa">ssh-dss  taco@taco.darkmay.cz</staci:ssh>
		<staci:access>
			<staci:permission>read</staci:permission>
		</staci:access>		

	</staci:user>

</staci:acl>
